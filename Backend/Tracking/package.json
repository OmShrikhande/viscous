{
  "name": "bus-tracking-server",
  "version": "1.0.0",
  "description": "Backend server for bus tracking and stop detection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:optimized": "node optimizedServer.js",
    "start:emergency": "node emergencyServer.js",
    "start:excel": "node excelServer.js",
    "dev": "nodemon server.js",
    "dev:optimized": "nodemon optimizedServer.js",
    "dev:emergency": "nodemon emergencyServer.js",
    "dev:excel": "nodemon excelServer.js",
    "setup": "node install-deps.js",
    "migrate": "node scripts/migrateToNewSerialLogic.js",
    "migrate:dry-run": "node scripts/migrateToNewSerialLogic.js --dry-run --backup",
    "test:migration": "node scripts/migrateToNewSerialLogic.js --dry-run",
    "test:serial-logic": "node test/testNewSerialLogic.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.14.1",
    "geolib": "^3.3.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
